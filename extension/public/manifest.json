{
    "manifest_version" : 3,
    "version" : "1.0.0",
    "name" : "Budget Breakdown",
    "action" : {
        "default_popup" : "index.html"
    },
    "permissions" : [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": ["https://www.amazon.com/*","http://localhost:3001/*"],
    "icons": {
        "32" : "icon32.png"
    }
  }
  